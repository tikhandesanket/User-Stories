
<table class="table">
	<thead class="thead-dark">
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Age</th>
			<th>Gender</th>
			<th>Country</th>
		</tr>
	</thead>
	<% users.each do |user| %>
		<tr>
			<td><%= user.firstname %></td>
			<td><%= user.lastname %></td>
			<td><%= user.age %></td>
			<td><%= user.gender %></td>
			<td><%= user.country %></td>
		</tr>
	<% end %>
</table>
