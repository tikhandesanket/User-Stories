
<table class="table">
	<thead class="thead-dark">
		  <th>Sr.</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Age</th>
			<th>Gender</th>
			<th>Country</th>
		</tr>
	</thead>
	<% users.each_with_index do |user,index| %>
		<tr>
		  <td><%= index+1%></td>
			<td><%= user.firstname %></td>
			<td><%= user.lastname %></td>
			<td><%= user.age %></td>
			<td><%= user.gender %></td>
			<td><%= user.country %></td>
		</tr>
	<% end %>
</table>
<%= will_paginate users %>